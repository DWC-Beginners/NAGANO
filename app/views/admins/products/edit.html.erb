<h1>商品編集</h1>
<%= form_with model:@product,local:true do |f| %>
<h4>商品画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_area :description %>
  <h4>ジャンル</h4>
  <%= f.select :genre_id, Genre.all.map { |genre| [genre.name, genre.id] }, {include_blank: '選択してください' }　%>
  <h4>税別価格</h4>
  <%= f.text_field :price %>
  <h4>販売ステータス</h4>
  <%= f.submit'変更を保存' %>
<% end %>