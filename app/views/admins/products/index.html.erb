<h1>商品一覧</h1>
<%= link_to "商品新規登録",new_admins_product_path %>
<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税別価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% @products.each do |product| %>
  <tbody>
    <tr>
      <th><%= product.id %></th>
      <th><%= product.name %></th>
      <th><%= product.price %></th>
      <th><%= product.genre.name %></th>
      <th><%= product.sale_status %></th>
    </tr>
  </tbody>
　<% end %>
</table>
<%= paginate @product %>